<head>

    <meta charset="{{ fn('language_attributes','charset') }}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-title" content="{{ fn('bloginfo','name') }} - {{ fn('bloginfo','description') }}">
    <link rel="profile" href="http://gmpg.org/xfn/11">
    <link rel="pingback" href="{{ fn('bloginfo','pingback_url') }} ">


    {% block head_content %}
    {% endblock %}

    <link href="https://fonts.googleapis.com/css?family=Montserrat:100,200,300,400,600,700" rel="stylesheet">
    <script src="https://use.fontawesome.com/0d8847f7d0.js"></script>
</head>
